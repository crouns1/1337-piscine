#include <stdlib.h>
#include <stdio.h>
char *ft_strdup(char *src) {
	int i;
	char *dest;
	i =0;
	while (src[i]) {
		dest = malloc(sizeof(char) * i);
		if (src == NULL) 
			return (NULL);
		i++;
	}
	i=0;
	while (dest[i]) {
		dest[i] = src[i];
		i++;
	}
	dest[i] = '\0';
	return dest;
}
int main () {
	char a[] = "hello";
	//free(a);
	printf("%s",ft_strdup(a));
}
